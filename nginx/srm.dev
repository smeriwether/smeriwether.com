server {
  listen 80;
  listen 443 ssl; # managed by Certbot
  ssl_certificate /etc/letsencrypt/live/srm.dev/fullchain.pem; # managed by Certbot
  ssl_certificate_key /etc/letsencrypt/live/srm.dev/privkey.pem; # managed by Certbot
  include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
  ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

  server_name srm.dev www.srm.dev;

  return 301 https://stephen.fyi$request_uri;
}
